{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "suppressExcessPropertyErrors": true,
    "typeRoots": [
           "node_modules/@types",
           "typings/custom"
       ]
  },
  "exclude":[
    "node_modules",
    "typings"
  ]
}
